{
  "name": "echoes-of-memories",
  "version": "1.0.0",
  "private": false,
  "type": "module",
  "description": "",
  "author": "Dakumisu",
  "license": "ISC",
  "sideEffects": false,
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "dev": "vite -c config/vite.config.js",
    "build": "vite build -c config/vite.config.js",
    "_preview": "vite preview -c config/vite.config.js",
    "preview": "npm run build -- -- --clearCache --manifestForceHash && npm run _preview",
    "preview:dev": "npm run build -- -m development -- --clearCache --manifestForceHash && npm run _preview -- -m development",
    "deploy:dev": "npm run build -- -m development -- --clearCache --manifestForceHash && netlify deploy --alias dev",
    "deploy:prod": "npm run build -- -- --clearCache --manifestForceHash && netlify deploy --prod",
    "deploy": "npm run deploy:prod",
    "clean": "rm -rf node_modules && rm -rf .cache"
  },
  "dependencies": {
    "@rollup/pluginutils": "^5.0.2",
    "@tweakpane/plugin-essentials": "^0.2.1",
    "@vitejs/plugin-vue": "^4.0.0",
    "generate-subtitles": "^1.0.6",
    "md5": "^2.3.0",
    "minimist": "^1.2.8",
    "netlify-cli": "^16.9.3",
    "quad-indices": "^2.0.1",
    "sass": "^1.57.1",
    "three": "^0.163.0",
    "tweakpane": "^4.0.3",
    "vite": "^4.0.4",
    "vtt-to-json": "^0.1.1",
    "vue": "^3.2.45",
    "vue-router": "^4.1.6",
    "word-wrapper": "^1.0.7",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@theatre/core": "^0.7.2",
    "@theatre/studio": "^0.7.2",
    "eslint": "^9.3.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-vue": "^9.26.0",
    "globals": "^15.3.0"
  },
  "imports": {
    "#app/*": "./app/*",
    "#webgl/*": "./webgl/*",
    "#utils/*": "./utils/*",
    "#assets/*": "./assets/*",
    "#config/*": "./config/*",
    "#/*": "./*"
  }
}
