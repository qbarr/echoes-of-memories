{
  "name": "echoes-of-memories",
  "version": "1.0.0",
  "private": false,
  "type": "module",
  "description": "",
  "author": "Dakumisu",
  "license": "ISC",
  "sideEffects": false,
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "dev": "vite -c config/vite.config.js",
    "build": "vite build -c config/vite.config.js",
    "_preview": "vite preview -c config/vite.config.js",
    "preview": "npm run build && npm run _preview",
    "preview:dev": "npm run build -- -m development && npm run _preview -- -m development",
    "deploy:dev": "npm run build -- -m development && netlify deploy --alias dev",
    "deploy:prod": "npm run build && netlify deploy --prod",
    "clean": "rm -rf node_modules && rm -rf .cache"
  },
  "dependencies": {
    "@rollup/pluginutils": "^5.0.2",
    "@tweakpane/plugin-essentials": "^0.2.1",
    "@vitejs/plugin-vue": "^4.0.0",
    "netlify-cli": "^16.9.3",
    "postprocessing": "^6.35.4",
    "sass": "^1.57.1",
    "three": "^0.163.0",
    "tweakpane": "^4.0.3",
    "vite": "^4.0.4",
    "vue": "^3.2.45",
    "vue-router": "^4.1.6"
  },
  "workspaces": [
    "packages/*"
  ],
  "imports": {
    "#app/*": "./app/*",
    "#webgl/*": "./webgl/*",
    "#utils/*": "./utils/*",
    "#assets/*": "./assets/*",
    "#config/*": "./config/*",
    "#/*": "./*"
  }
}
